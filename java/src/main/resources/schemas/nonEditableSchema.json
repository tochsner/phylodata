{
  "$ref": "#/$defs/NonEditablePaperWithExperiment",
  "$defs": {
    "NonEditablePaperWithExperiment": {
      "title": "NonEditablePaperWithExperiment",
      "description": "This structure contains all experiment data which is\n    computed by the pipeline and must not be changed manually.",
      "type": "object",
      "properties": {
        "paper": {
          "$ref": "#/$defs/NonEditablePaper"
        },
        "experiment": {
          "$ref": "#/$defs/NonEditableExperiment"
        },
        "files": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/File"
          }
        },
        "trees": {
          "$ref": "#/$defs/Trees"

        },
        "evolutionaryModel": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/EvolutionaryModelComponent"
          }
        },
        "metadata": {
          "$ref": "#/$defs/Metadata"
        }
      },
      "required": [
        "paper",
        "experiment",
        "files",
        "trees",
        "evolutionaryModel",
        "metadata"
      ]
    },
    "NonEditablePaper": {
      "title": "NonEditablePaper",
      "type": "object",
      "properties": {
        "doi": {
          "type": "string"
        }
      },
      "required": [
        "doi"
      ]
    },
    "NonEditableExperiment": {
      "title": "NonEditableExperiment",
      "type": "object",
      "properties": {
        "humanReadableId": {
          "type": "string"
        },
        "origin": {
          "type": "string"
        },
        "uploadDate": {
          "type": "string",
          "format": "date"
        },
        "version": {
          "type": "integer"
        },
        "license": {
          "type": "string",
          "default": "CC0"
        },
        "id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        }
      },
      "required": [
        "humanReadableId",
        "origin",
        "uploadDate",
        "version"
      ]
    },
    "File": {
      "title": "File",
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "$ref": "#/$defs/FileType"
        },
        "sizeBytes": {
          "type": "integer"
        },
        "md5": {
          "type": "string"
        },
        "isPreview": {
          "type": "boolean",
          "default": false
        },
        "localPath": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        },
        "remotePath": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        }
      },
      "required": [
        "name",
        "type",
        "sizeBytes",
        "md5"
      ]
    },
    "FileType": {
      "title": "FileType",
      "enum": [
        "beast2Configuration",
        "beast2PosteriorLogs",
        "codephyModel",
        "phyloDataExperiment",
        "posteriorTrees",
        "summaryTree",
        "unknown"
      ]
    },
    "Trees": {
      "title": "Trees",
      "type": "object",
      "properties": {
        "numberOfTrees": {
          "type": "integer"
        },
        "numberOfTips": {
          "type": "integer"
        },
        "ultrametric": {
          "type": "boolean"
        },
        "timeTree": {
          "type": "boolean"
        },
        "rooted": {
          "type": "boolean"
        },
        "ccd1Entropy": {
          "type": "number"
        },
        "treeEss": {
          "type": "integer"
        },
        "ccd0MapTree": {
          "type": "string"
        },
        "hipstrTree": {
          "type": "string"
        },
        "leafToSampleMap": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "averageRootAge": {
          "type": "number"
        }
      },
      "required": [
        "numberOfTrees",
        "numberOfTips",
        "ultrametric",
        "timeTree",
        "rooted",
        "ccd1Entropy",
        "treeEss",
        "ccd0MapTree",
        "hipstrTree",
        "leafToSampleMap",
        "averageRootAge"
      ]
    },
    "EvolutionaryModelComponent": {
      "title": "EvolutionaryModelComponent",
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "$ref": "#/$defs/ModelType"
        },
        "parameters": {
          "type": "object"
        }
      },
      "required": [
        "name",
        "type",
        "parameters"
      ]
    },
    "ModelType": {
      "title": "ModelType",
      "enum": [
        "clockModel",
        "other",
        "substitutionModel",
        "treeLikelihood",
        "treePrior"
      ]
    },
    "Metadata": {
      "title": "Metadata",
      "type": "object",
      "properties": {
        "evoDataPipelineVersion": {
          "type": "string"
        }
      },
      "required": [
        "evoDataPipelineVersion"
      ]
    }
  }
}